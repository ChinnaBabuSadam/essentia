RBS

{
    "amount": {
        "properties": {
            "amount": {
                "type": "string",
                "x-samplevalue": "10.00"
            },
            "code": {
                "type": "string",
                "x-samplevalue": "USD"
            }
        },
        "title": "amount",
        "type": "object",
        "x-has-customfields": false
    },
    "createPayment": {
        "id": "createPayment",
        "properties": {
            "accounting_systemid": {
                "type": "string",
                "x-samplevalue": "zDmNq"
            },
            "amount": {
                "type": "amount"
            },
            "clientid": {
                "format": "int32",
                "type": "integer",
                "x-samplevalue": 1758507
            },
            "creditid": {
                "type": "string",
                "x-samplevalue": "545454"
            },
            "date": {
                "format": "date",
                "type": "string",
                "x-mask": "yyyy-MM-dd",
                "x-samplevalue": "2013-12-10"
            },
            "from_credit": {
                "type": "boolean",
                "x-samplevalue": false
            },
            "gateway": {
                "type": "string",
                "x-samplevalue": "The payment processor used if any"
            },
            "id": {
                "format": "int32",
                "type": "integer",
