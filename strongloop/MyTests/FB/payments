                "x-samplevalue": 10865326
            },
            "invoiceid": {
                "format": "int32",
                "type": "integer",
                "x-samplevalue": 2010190
            },
            "logid": {
                "format": "int32",
                "type": "integer",
                "x-samplevalue": 10865326
            },
            "note": {
                "type": "string",
                "x-samplevalue": "This is note"
            },
            "orderid": {
                "type": "string",
                "x-samplevalue": "454455"
            },
            "overpaymentid": {
                "format": "int32",
                "type": "integer",
                "x-samplevalue": 545
            },
            "transactionid": {
                "format": "int32",
                "type": "integer",
                "x-samplevalue": 54545
            },
            "type": {
                "type": "string",
